  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Lunch Chrome React Run",
      "url": "http://localhost:3000",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src",
      "userDataDir": false
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceRoot}"
    }
  ]
}
